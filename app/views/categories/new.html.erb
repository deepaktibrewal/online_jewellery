<body>
  <header class="header">
    <div class="container ">
      <ul class="nav nav-pills navbar-right">
        <li role="presentation"><%= link_to("<< Back To List",categories_path,class: 'back-link') %></li>
        <li role="presentation"><%= link_to("Logout", access_logout_path) %></li>
      </ul>
    </div>
  </header>
<div class="Categories new container ">
  <h2> Create Category</h2>
  <div >
    <%= form_for(@category) do |f| %>
      <%= error_messages_for(f.object) %>
      <table summary="Category form fields" class="form-group">
        <tr >
          <th>Name:</th>
          <td><%= f.text_field(:name) %></td>
        </tr>
      </table>

      <div class="form-buttons">
        <%= f.submit("Create Category") %>
      </div>

      <% end %>
  </div>
</div>
</body>
